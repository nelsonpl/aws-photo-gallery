{
  "CallerReference": "my-unique-id",
  "Comment": "My first CloudFront distribution",
  "DefaultCacheBehavior": {
    "TargetOriginId": "<origin-id>",
    "ForwardedValues": {
      "QueryString": false,
      "Cookies": {
        "Forward": "all"
      }
    },
    "ViewerProtocolPolicy": "allow-all",
  },
  "Enabled": true,
  "Origins": {
    "Quantity": 1,
    "Items": [
      {
        "Id": "<origin-id>",
        "DomainName": "<nome-do-seu-bucket>.s3.amazonaws.com"
      }
    ]
  }
}